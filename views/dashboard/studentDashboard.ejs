<div class="dashboard-container">
  <h2>Welcome, <%= isLoginm.name %>!</h2>
  <p class="subtitle">Here are your enrolled courses:</p>

  <% if (courses.length === 0) { %>
    <p>You havenâ€™t enrolled in any courses yet.</p>
  <% } else { %>
    <div class="courses-grid">
      <% courses.forEach(course => { %>
        <div class="course-card">
          <h3><%= course.title %></h3>
          <p><%= course.description %></p>
          <a href="/student/course/<%= course.id %>" class="btn-view">View Course</a>
